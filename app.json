{
  "expo": {
    "name": "GoNext",
    "slug": "egonext",
    "version": "1.0.0",
    "scheme": "egonext",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.egonext.app"
    },
    "android": {
      "package": "com.egonext.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-image-picker",
        {
          "photosPermission": "Приложение использует галерею для прикрепления фотографий к местам и поездкам.",
          "cameraPermission": "Приложение использует камеру для съёмки фотографий мест и поездок."
        }
      ],
      ["expo-build-properties",{"android":{"compileSdkVersion":35,"targetSdkVersion":35,"buildToolsVersion":"35.0.0"}}]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c9b28b01-62b0-44f7-abc5-d2a4334566be"
      }
    }
  }
}
