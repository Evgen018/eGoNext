{
  "expo": {
    "name": "GoNext",
    "slug": "egonext",
    "version": "1.0.0",
    "scheme": "egonext",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.egonext.app"
    },
    "android": {
      "package": "com.egonext.app"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-image-picker",
        {
          "photosPermission": "Приложение использует галерею для прикрепления фотографий к местам и поездкам.",
          "cameraPermission": "Приложение использует камеру для съёмки фотографий мест и поездок."
        }
      ]
    ]
  }
}
